{
	"name": "drinktracker",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"bulma": "^0.6.1",
		"date-fns": "^1.29.0",
		"idb-keyval": "^2.3.0",
		"lodash": "^4.17.4",
		"moment": "^2.20.1",
		"react": "^16.0.0",
		"react-datetime": "^2.11.1",
		"react-dom": "^16.0.0",
		"react-scripts": "^1.0.17",
		"react-tappable": "^1.0.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"debug-test":
			"node --inspect-brk ./node_modules/react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"deploy":
			"yarn build && scp -r build root@drinktracker.tweeres.ca:/var/www/drinktracker && scp etc/drinktracker root@drinktracker.tweeres.ca:/etc/nginx/sites-available && ssh root@drinktracker.tweeres.ca 'nginx -s reload'"
	},
	"devDependencies": {
		"prettier-eslint": "^8.6.2"
	}
}
