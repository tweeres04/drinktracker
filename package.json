{
	"name": "drinktracker",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"autotrack": "^2.4.1",
		"bulma": "^0.6.1",
		"classnames": "^2.2.5",
		"date-fns": "^1.29.0",
		"idb-keyval": "^2.3.0",
		"lodash": "^4.17.4",
		"moment": "^2.20.1",
		"react": "^16.0.0",
		"react-datetime": "^2.11.1",
		"react-dom": "^16.0.0",
		"react-scripts": "^1.0.17",
		"react-tappable": "^1.0.2",
		"source-map-explorer": "^1.5.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"debug-test": "node --inspect-brk ./node_modules/react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"deploy": "yarn build && ssh root@drinktracker.tweeres.ca 'rm -r /var/www/drinktracker'; scp -r build root@drinktracker.tweeres.ca:/var/www/drinktracker",
		"deployconfig": "scp etc/drinktracker root@drinktracker.tweeres.ca:/etc/nginx/sites-available && ssh root@drinktracker.tweeres.ca 'nginx -s reload'",
		"deploysurge": "yarn build && surge --domain https://drinktracker.surge.sh",
		"analyze": "source-map-explorer build/static/js/main.*",
		"storybook": "start-storybook -p 9009 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^3.3.9",
		"@storybook/addon-links": "^3.3.9",
		"@storybook/addons": "^3.3.9",
		"@storybook/react": "^3.3.9",
		"babel-core": "^6.26.0",
		"prettier-eslint": "^8.6.2"
	}
}
